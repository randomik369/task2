{
  "env": {
    "browser": true,
    "es2020": true,
    "jquery": true
  },
  "extends": [
    "airbnb-base",
    "plugin:fsd/all"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "fsd"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "max-len": 0,
    "linebreak-style": "off",
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": [
      "error", {
         "devDependencies": true
      }
  ]
  },
  "parser": "babel-eslint"
}